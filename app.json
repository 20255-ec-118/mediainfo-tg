{
    "name": "MediaInfoBot",
    "description": "Multiutility Telegram bot by Bunny",
    "repository": "https://github.com/bunnykek/MediaInfoBot",
    "logo": "https://imgur.com/UsUog2Z.png",
    "keywords": [
        "telegram",
        "telegram-bot",
        "mediainfo",
        "spectrogram-bot"
    ],
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Get your api_id from https://my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get your api_hash from https://my.telegram.org/apps",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Get your bot's token from https://telegram.me/BotFather",
            "required": true
        },
        "GDTOT_PHP": {
            "description": "Get it from GDTOT cookies",
            "required": false
        },
        "GDTOT_CRYPT": {
            "description": "Get it from GDTOT cookies",
            "required": false
        },
        "APPDRIVE_PHP": {
            "description": "Get it from AppDrive cookies",
            "required": false
        },
        "APPDRIVE_MD": {
            "description": "Get it from AppDrive cookies",
            "required": false
        },
        "TOKEN_JSON": {
            "description": "token.json contents",
            "required": false
        },
        "OWNER": {
            "description": "Bot owner Telegram ID",
            "required": false
        }
    }
}
